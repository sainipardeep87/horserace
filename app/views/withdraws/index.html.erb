<h1>Listing withdraws</h1>

<table>
  <tr>
    <th>Amount</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @withdraws.each do |withdraw| %>
  <tr>
    <td><%= withdraw.amount %></td>
    <td><%= link_to 'Show', withdraw %></td>
    <td><%= link_to 'Edit', edit_withdraw_path(withdraw) %></td>
    <td><%= link_to 'Destroy', withdraw, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Withdraw', new_withdraw_path %>
